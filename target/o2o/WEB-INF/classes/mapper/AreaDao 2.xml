<mapper namespace="com.imooc.o2o.dao.AreaDao">
    <select id="queryArea" resultType="com.imooc.o2o.entity.Area">
        select area_id, area_name, priority, create_time, last_edit_time
        from tb_area
        order by priority desc
    </select>
</mapper>